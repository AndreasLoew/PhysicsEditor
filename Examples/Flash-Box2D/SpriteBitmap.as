package {	import flash.display.Bitmap;	import flash.display.Loader;	import flash.display.Sprite;	import flash.events.*;	import flash.net.URLRequest;	import flash.utils.getDefinitionByName;	class SpriteBitmap extends Sprite {		public function SpriteBitmap(name:String) {			// Grab the bitmap from the compiled resources			var artClass:Class = getDefinitionByName(name) as Class;			// creat a display object to add to the stage			var bitmap:Bitmap = new Bitmap(new artClass(0, 0), "auto", true);			addChild(bitmap);		}		private function imageFailed(event:IOErrorEvent):void 		{		}	}}